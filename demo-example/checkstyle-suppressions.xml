<?xml version="1.0"?>
<!DOCTYPE suppressions PUBLIC
        "-//Checkstyle//DTD SuppressionFilter Configuration 1.0//EN"
        "https://checkstyle.org/dtds/suppressions_1_0.dtd">

<suppressions>
    <suppress files=".*properties" checks="RegexpSingleline"/>

    <suppress files=".*java" checks="JavadocPackage"/>
    <suppress files=".*java" checks="DesignForExtension"/>
    <suppress files=".*java" checks="JavadocVariable"/>
    <suppress files=".*java" checks="MissingJavadocMethod"/>

    <!-- TESTS -->
    <suppress files="src[\\/]test[\\/]java[\\/]com[\\/]onlyoffice[\\/].*" checks="MissingJavadocMethod"/>
    <suppress files="src[\\/]test[\\/]java[\\/]com[\\/]onlyoffice[\\/].*" checks="JavadocVariable"/>
    <suppress files="src[\\/]test[\\/]java[\\/]com[\\/]onlyoffice[\\/].*" checks="JavadocVariable"/>
    <suppress files="src[\\/]test[\\/]java[\\/]com[\\/]onlyoffice[\\/].*" checks="LineLength"/>
    <suppress files="src[\\/]test[\\/]java[\\/]com[\\/]onlyoffice[\\/].*" checks="OperatorWrap"/>
    <suppress files="src[\\/]test[\\/]java[\\/]com[\\/]onlyoffice[\\/].*" checks="MagicNumber"/>
</suppressions>