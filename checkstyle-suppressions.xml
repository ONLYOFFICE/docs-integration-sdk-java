    <suppress files=".*java" checks="JavadocPackage"/>


    <suppress files="src[\\/]main[\\/]java[\\/]com[\\/]onlyoffice[\\/]model[\\/].*" checks="MissingJavadocMethod"/>
    <suppress files="src[\\/]main[\\/]java[\\/]com[\\/]onlyoffice[\\/]model[\\/].*" checks="JavadocVariable"/>
